datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Company {
  id            Int      @id @default(autoincrement())
  rpoId         Int      @unique
  ico           String   @unique
  name          String
  municipality  String
  establishment DateTime
  termination   DateTime?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}